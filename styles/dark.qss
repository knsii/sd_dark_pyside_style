QScrollArea,QScrollArea > QWidget,QScrollArea > QWidget > QWidget{background:transparent}
QAbstractButton::menu-indicator:enabled,QAbstractButton::menu-arrow:enabled{image:url(dark/caret-down-solid.svg);subcontrol-position:right center;top:0}
QAbstractButton::menu-indicator:enabled:checked,QAbstractButton::menu-indicator:enabled:pressed,QAbstractButton::menu-indicator:!enabled,QAbstractButton::menu-arrow:enabled:checked,QAbstractButton::menu-arrow:enabled:pressed,QAbstractButton::menu-arrow:!enabled{image:url(dark/caret-down-solid.svg)}
QAbstractSpinBox{padding-left:10px;height:16px}
QAbstractSpinBox::down-button,QAbstractSpinBox::up-button{background-repeat:no-repeat;border:none;width:12px;padding:3px;subcontrol-origin: padding}
QAbstractSpinBox::down-button{background-position:center;background-image:url(dark/caret-down-solid.svg)}
QAbstractSpinBox::up-button{background-position:center;background-image:url(dark/caret-up-solid.svg)}
QAbstractSpinBox::down-button:enabled:hover,QAbstractSpinBox::up-button:enabled:hover{background-color:#1a1a1a;color:white;border:0 solid transparent}
