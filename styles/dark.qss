


*{outline:none;font-family:"Segoe UI"}
QCheckBox,QListView,QPushButton,QRadioButton,QTabBar,QToolBar,QToolBox,QTreeView{icon-size:14px}
QMainWindow,QWidget:window{background-color:#333333;color:#555555}
QDockWidget{background-color:#333333;border:4 solid #333333}
QWidget{background:#333333;color:#c8c8c8}
QFrame{color:#c8c8c8}
QFrame[frameShape="3"]{background-color:#5a5a5a;border-color:#808080 transparent #404040 transparent;border-style:solid;border-width:1px;padding:3px}
QFrame[frameShape="4"],QFrame[frameShape="5"]{background:#2b2b2b;border:none;color:transparent;qproperty-frameShadow:"Plain"}
QFrame[frameShape="4"]{min-height:1px;max-height:1px;margin:4px 0}
QFrame[frameShape="5"]{min-width:1px;max-width:1px;margin:0 4px}
QComboBox{background:#262626;color:#c8c8c8;border:1px solid #444444;border-radius:3px;padding:0px 0px 0px 4px;height:20px}
QComboBox:hover{color:#1a8dff}
QComboBox:disabled{background-color:#3f3f3f;color:#141414;border:1px solid #4d4d4d}
QComboBox:checked{background-color:#1a1a1a;color:#1a8dff}
QComboBox::drop-down{color:#c8c8c8;border:none}
QComboBox::down-arrow{image:url('dark/caret-down-solid.svg')}
/* QComboBox::down-arrow:disabled{image:url(':/pfx/editor/gui/images/droparrow_disable.svg')} */
QComboBox QAbstractItemView{border:1px solid #141414;background-color:#4d4d4d;selection-background-color:#333333;selection-color:#1a8dff;padding:1px;outline:none}
QComboBox[isSupported="false"]{background-color:#FCB917;color:#808080;font-style:italic}
QComboBox[editable="true"]{padding:0;border:0;border-radius:3px}
QComboBox[editable="true"]::drop-down{border:1px solid #444444;border-radius:3px}
QLabel:disabled{color:#777777}
