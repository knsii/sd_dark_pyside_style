QListView{background-color:transparent;}
QTreeView{background-color:transparent;alternate-background-color:#333333;show-decoration-selected:1;selection-background-color:transparent;outline:none;border:none}
QTreeView::branch,QTreeView::item{padding:1px 0;outline:none}
QTreeView::branch:has-children{border-image:none}
QTreeView::branch:has-children:closed{image:url(dark/caret-right-solid.svg);padding:4px;subcontrol-origin: padding}
