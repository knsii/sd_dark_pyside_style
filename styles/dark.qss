QAbstractItemView::branch:enabled:selected{background-color:#555555;border:0 solid transparent;color:white}
QHeaderView::section{background:#4d4d4d;border-color:#252525;border-style:solid;border-width:1px 0px 0px 1px;color:#c8c8c8;padding:0px 3px}

QHeaderView::down-arrow{image:url(dark/caret-down-solid.svg);padding:3px;min-height:16px;min-width:16px}
QHeaderView::up-arrow{image:url(dark/caret-up-solid.svg);padding:3px;min-height:16px;min-width:16px}
QDockWidget{color:#c8c8c8;font-weight:bold}
QDockWidget::title{background:#252525;color:#c8c8c8;border-bottom:0px solid black;padding-left:4px;padding-top:5px}
QGroupBox{border:none;margin:18px 0 0 0;padding:2px 10px;font-size:14px}
QGroupBox::title{background:transparent;color:#e5e5e5;left:0;padding:2px 10px;top:-16px}
QGroupBox QGroupBox{border:none;margin:18px 0 0 0;padding:2px 10px;font-size:11px}
QGroupBox#bakerView2DGroupBox{padding:2px 0px 0px 0px}

QMenu{color:#c8c8c8;background:#4d4d4d;border-left:1 solid #333333;border-bottom:1 solid #333333;font-size:11px;}
