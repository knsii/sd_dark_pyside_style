QDockWidget {border:none;outline:none;}

QTabBar{background:#1b1b1b; qproperty-drawBase:0;}
QTabBar{border-bottom:none}
QTabBar::tab{background:#1b1b1b;color:#aaaaaa;border-right:0px solid #141414;padding:5px 8px}
